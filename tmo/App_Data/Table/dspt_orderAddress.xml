<?xml version="1.0" encoding="utf-8"?><DataSourceSchema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><Id>dspt_orderAddress</Id><IsAutoNewLine>false</IsAutoNewLine><UserSqlCommands /><PageSize>10</PageSize><References /><LookupFields /><PrimaryKeys><string>ID</string></PrimaryKeys><InsertCommand><CommandText>INSERT INTO pt_shippingAddress ( [Province], [City], [Region], [Supply], [Detail], [Receiver], [Phone], [IsDefault], [Summary], [Creater], [CreateTime], [Updater], [UpdateTime]) VALUES (@Province,@City,@Region,@Supply,@Detail,@Receiver,@Phone,@IsDefault,@Summary,@Creater,@CreateTime,@Updater,@UpdateTime);SELECT * FROM pt_shippingAddress WHERE [ID]=SCOPE_IDENTITY()</CommandText><CommandType>Text</CommandType></InsertCommand><UpdateCommand><CommandText>UPDATE pt_shippingAddress SET  [Province]=@Province, [City]=@City, [Region]=@Region, [Supply]=@Supply, [Detail]=@Detail, [Receiver]=@Receiver, [Phone]=@Phone, [IsDefault]=@IsDefault, [Summary]=@Summary, [Creater]=@Creater, [CreateTime]=@CreateTime, [Updater]=@Updater, [UpdateTime]=@UpdateTime WHERE  [ID]=@pk_ID</CommandText><CommandType>Text</CommandType></UpdateCommand><DeleteCommand><CommandText>DELETE FROM pt_shippingAddress WHERE  [ID]=@pk_ID</CommandText><CommandType>Text</CommandType></DeleteCommand><SelectCommand><CommandText>select sa.ID,sa.Receiver,sa.Phone,sa.IsDefault,p.REGION_NAME Province,c.REGION_NAME City,r.REGION_NAME Region,(p.REGION_NAME+c.REGION_NAME+r.REGION_NAME+sa.supply) Detaillocation 
