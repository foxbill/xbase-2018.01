 [Amount],
[Attrs])
 VALUES (
@DetailsId,
@OrderNo,
@GoodsId,
@GoodsName,
@Model,
@Price,
@OrgPrice,
@Count,
@Count*@Price,
@Attrs);

update ma_order 
   set
      PayAmount=(select sum(Amount)  from ma_order_details where OrderNo=@OrderNo)
Where OrderNo=@OrderNo;


select * from ma_order_details    where
 [OrderNo]=@OrderNo and
 [GoodsId]=@GoodsId and
 [GoodsName]=@GoodsName and
 [Model]=@Model and
 [Attrs]=@Attrs;



