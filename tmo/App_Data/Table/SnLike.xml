      select * from 
         sn_like 
      where 
          TargetId='3de93e7c-bb55-45e0-8d4f-35f902fc9e16' 
          and TargetType='sn_blog'  
          and LikerId='af00335a-1d49-46c5-a066-b0ed020c760b'
)
begin
   delete from sn_like where
          TargetId='3de93e7c-bb55-45e0-8d4f-35f902fc9e16' 
          and TargetType='sn_blog'  
          and LikerId='af00335a-1d49-46c5-a066-b0ed020c760b'
end
else 
begin
   INSERT INTO sn_like (
     [Id],
     [LikerId],
     [TargetType],
     [TargetId],
     [CreateTime])
    VALUES (
   @Id,
   @LikerId,
   @TargetType,
   @TargetId,
   @CreateTime);
end

